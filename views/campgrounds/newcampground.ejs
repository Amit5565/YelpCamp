<% include ../partials2/header %>
<div class="container">
  <div class="row">
      <h2 style="text-align:center">Create A New CampGround</h2>
  <div style="width:30%;margin:25px auto">

<form action="/campgrounds" method="POST">
  <div class="form-group">
    <input type="text" name="name" class="form-control" placeholder="Name">
  </div>

    <div class="form-group">
      <input type="text" name="img" class="form-control" placeholder="ImgURL">
    </div>
     <div class="form-group">
      <input type="text"  class="form-control" name="description" placeholder="Description">
     </div>

     <div class="form-group">
      <input type="number"  class="form-control" name="price" placeholder="Price" min="0.01" step="0.01">
     </div>
        <div class="form-group">
          <button class="btn btn-primary btn-block">Submit</button>
        </div>
</form>
</div>
</div>
</div>
<% include ../partials2/footer %>
